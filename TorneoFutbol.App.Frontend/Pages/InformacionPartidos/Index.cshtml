@page
@model TorneoFutbol.App.Frontend.Pages.InformacionPartidos.IndexModel
@{
}
<h1>Listado Informacion de los Partidos</h1>


<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Minuto</th>
            <th scope="col">Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var informacionpartido in Model.informacionpartido)
        {
            <tr>
                <td>@informacionpartido.Minuto</td>
                <td><a class="btn btn-secondary" asp-page="./Details" asp-route-id="@informacionpartido.Id">Detalle</a></td>
            </tr>
        }
        
    </tbody>
    
</table>
<a class="btn btn-primary" asp-page="./Create">Crear InformacionPartido</a>
