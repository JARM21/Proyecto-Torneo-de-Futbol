@page
@model TorneoFutbol.App.Frontend.Pages.Municipios.IndexModel
@{
}
<h1>Listado de Municipios</h1>


<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var municipio in Model.municipio)
        {
            <tr>
                <td>@municipio.Nombre</td>
                <td><a class="btn btn-secondary" asp-page="./Details" asp-route-id="@municipio.Id">Detalle</a></td>
            </tr>
        }
    </tbody>
</table>
