@page
@model TorneoFutbol.App.Frontend.Pages.DirectoresTecnicos.IndexModel
@{
}
<h1>Listado de DirectoresTecnicos</h1>


<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Documento</th>
            <th scope="col">Telefono</th>
            
            <th scope="col">Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var directortecnico in Model.directortecnico)
        {
            <tr>
                <td>@directortecnico.Nombre</td>
                <td>@directortecnico.Documento</td>
                <td>@directortecnico.Telefono</td>
                <td><a class="btn btn-secondary" asp-page="./Details" asp-route-id="@directortecnico.Id">Detalle</a></td>
            </tr>
        }
        
    </tbody>
    
</table>
<a class="btn btn-primary" asp-page="./Create">Crear DirectorTecnico</a>
